<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>AgregarBodega</servlet-name>
        <servlet-class>servlet.AgregarBodega</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarBodega</servlet-name>
        <servlet-class>servlet.EliminarBodega</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModificarBodega</servlet-name>
        <servlet-class>servlet.ModificarBodega</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AgregarFarmaceutico</servlet-name>
        <servlet-class>servlet.AgregarFarmaceutico</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarFarmaceutico</servlet-name>
        <servlet-class>servlet.EliminarFarmaceutico</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModificarFarmaceutico</servlet-name>
        <servlet-class>servlet.ModificarFarmaceutico</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AgregarMedicamento</servlet-name>
        <servlet-class>servlet.AgregarMedicamento</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarMedicamento</servlet-name>
        <servlet-class>servlet.EliminarMedicamento</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModificarMedicamento</servlet-name>
        <servlet-class>servlet.ModificarMedicamento</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AgregarBodega</servlet-name>
        <url-pattern>/AgregarBodega</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarBodega</servlet-name>
        <url-pattern>/EliminarBodega</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModificarBodega</servlet-name>
        <url-pattern>/ModificarBodega</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AgregarFarmaceutico</servlet-name>
        <url-pattern>/AgregarFarmaceutico</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarFarmaceutico</servlet-name>
        <url-pattern>/EliminarFarmaceutico</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModificarFarmaceutico</servlet-name>
        <url-pattern>/ModificarFarmaceutico</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AgregarMedicamento</servlet-name>
        <url-pattern>/AgregarMedicamento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarMedicamento</servlet-name>
        <url-pattern>/EliminarMedicamento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModificarMedicamento</servlet-name>
        <url-pattern>/ModificarMedicamento</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Bodega/AgregarBodega.jsp</welcome-file>
    </welcome-file-list>
</web-app>
